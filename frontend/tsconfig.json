{
  "compilerOptions": {
      "target": "es2017",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "downlevelIteration": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "baseUrl": ".",
      "paths": {
          "@/*": ["./src/*"]
      }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/texteditor/components.tsx", "src/middleware.ts"],
  "exclude": ["node_modules"]

  
}

